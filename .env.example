# Supabase Configuration (get from https://app.supabase.com/project/YOUR_PROJECT/settings/api)
VITE_SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
# NEVER expose service role key in frontend!

# Stripe Configuration (get from https://dashboard.stripe.com/apikeys)
# Use test keys for development!
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here
# Secret keys go in backend/environment variables only, never in frontend!

# Application Configuration
VITE_APP_URL=http://localhost:5173
VITE_APP_NAME="Legally Legit AI"
VITE_APP_ENV=development

# Optional: AI Integration
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Analytics
VITE_GA_ID=G-XXXXXXXXXX

# Support
VITE_SUPPORT_EMAIL=support@legallylegit.ai
